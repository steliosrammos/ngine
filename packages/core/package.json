{
  "name": "@ngine/core",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "extract": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file src/lang.json --flatten true",
    "compile": "formatjs compile src/lang.json --out-file src/translations/en.json"
  },
  "devDependencies": {
    "@formatjs/cli": "^6.2.4",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/lodash.uniqby": "^4.7.9",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "eslint": "^8.54.0",
    "eslint-plugin-formatjs": "^4.11.3",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.2.2",
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/styled-system": "^2.9.2",
    "@chakra-ui/system": "^2.6.2",
    "@chakra-ui/theme": "^3.3.1",
    "@chakra-ui/utils": "^2.0.14",
    "@emoji-mart/data": "^1.1.2",
    "@emoji-mart/react": "^1.1.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@getalby/bitcoin-connect-react": "^2.4.3",
    "@noble/hashes": "^1.3.3",
    "@nostr-dev-kit/ndk": "^2.3.0",
    "@scure/base": "^1.1.5",
    "@tanstack/react-query": "^5.13.4",
    "@void-cat/api": "^1.0.10",
    "bech32": "^2.0.0",
    "emoji-mart": "^5.5.2",
    "framer-motion": "^10.16.16",
    "jotai": "^2.6.0",
    "light-bolt11-decoder": "^3.0.0",
    "lodash.uniqby": "^4.7.0",
    "nostr-tools": "^1.17.0",
    "react-intersection-observer": "^9.5.3",
    "react-intl": "^6.5.5",
    "react-markdown": "^9.0.1",
    "react-qr-code": "^2.0.12",
    "remark-gfm": "^4.0.0"
  }
}
